local extra = serv:Channel("Extras")

extra:Button("Rejoin", function()
print("Rejoining!")
wait(0.1)
DiscordLib:Notification("Notification", "Attempting to Rejoin...", "Uhhh...")

local ts = game:GetService("TeleportService")

local p = game:GetService("Players").LocalPlayer

 

ts:Teleport(game.PlaceId, p)

end)


extra:Button("Server Hop", function()

wait(0.1)
DiscordLib:Notification("Notification", "Attempting to Server Hop...", "Uhhh...")
game:GetService("TeleportService"):Teleport(gameid, game:GetService("Players").LocalPlayer)

end)

