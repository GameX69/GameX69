--GDHC TOOL SCRIPT








































wait(1)
local CoreGui = game:GetService("StarterGui") 

CoreGui:SetCore("SendNotification", {
    Title = "GDHC TOOL",
    Text = "Hello! We're checking if your IP is whitelisted",
    Duration = 5, 
})

wait(5)
local CoreGui = game:GetService("StarterGui") 

CoreGui:SetCore("SendNotification", {
    Title = "GDHC TOOL",
    Text = "IP Whitelisted! Loading Script...",
    Duration = 5, 
})

wait(1.8)



local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("GDHC TOOL")

local serv = win:Server("Tool", "")

local ourscripts = serv:Channel("GDHC Scripts")

ourscripts:Label("Audio ID Grabbing")

ourscripts:Button("Edge's Audio Logger", function()
print("Executing Edge's Audio Logger...")
loadstring(game:HttpGet('https://pastebin.com/raw/mKPRwVSW'))();
DiscordLib:Notification("Edge's ID Logger", "Edge's Audio Logger was Loaded!", "Okay!")
print("Edge's Audio Logger was Loaded!")

end)

ourscripts:Seperator()

ourscripts:Label("Chat Scripts")


local other = serv:Channel("Other scripts")

other:Label("Swagmode Stuff")

other:Button("Swagmode", function()
print("Executing Swagmode..")
loadstring(game:HttpGet("https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002"))()
