end)

other:Button("Faded Crash", function()
print("Executing Faded Crash")
DiscordLib:Notification("Faded Crash", "It looks like Faded's crash method was Patched...", ":c")
print("Error: Faded crash didn't Load..")

end)


other:Seperator()

